@model Tenant

@{ 
    
}
<div class="card">
    <div class="card-header">
        <h3>Ürün Detayı</h3>
    </div>
    <div class="card-body">
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>@Html.DisplayNameFor(m => m.Id)</th>
                    <th>@Html.DisplayNameFor(m => m.Title)</th>
                    <th>@Html.DisplayNameFor(m => m.FullName)</th>
                    <th>@Html.DisplayNameFor(m => m.Address)</th>
                    <th>@Html.DisplayNameFor(m => m.CityId)</th>
                    <th>@Html.DisplayNameFor(m => m.CountryId)</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>@Html.DisplayFor(m => m.Id)</td>
                    <td>@Html.DisplayFor(m => m.Title)</td>
                    <td>@Html.DisplayFor(m => m.FullName)</td>
                    <td>@Html.DisplayFor(m => m.Address)</td>
                    <td>@Html.DisplayFor(m => m.CityId)</td>
                    <td>@Html.DisplayFor(m => m.CountryId)</td>
                </tr>
            </tbody>
        </table>
    </div>
    <div class="card-footer">
        <div asp-validation-summary="All" class="text-danger"></div>
    </div>
</div>