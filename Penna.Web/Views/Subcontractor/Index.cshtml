                                <!--begin::Label-->
                                <label class="required fs-6 fw-bold mb-2">Vergi Dairesi</label>
                                <!--end::Label-->
                                <!--begin::Input-->
                                <input type="text" asp-for="CurrentAccount.TaxOffice" class="form-control form-control-solid" placeholder="Vergi dairesi giriniz" />
                                <!--end::Input-->
                            </div>
                            <!--end::Col TaxOffice-->
                        </div>
                        <!--end::Input group TaxId & TaxOffice-->
                        <!--begin::Input group Phone1 & Phone2-->
                        <div class="row g-9 mb-7">
                            <!--begin::Input group Phone1-->
                            <div class="col-md-6 fv-row">
                                <!--begin::Label-->
                                <label class="required fs-6 fw-bold mb-2">Telefon-1</label>
                                <!--end::Label-->
                                <!--begin::Input-->
                                <input type="text" asp-for="CurrentAccount.Phone1" class="form-control form-control-solid" placeholder="Telefon no giriniz" />
                                <!--end::Input-->
                            </div>
                            <!--end::Input group Phone1-->
                            <!--begin::Input group Phone2-->
                            <div class="col-md-6 fv-row">
                                <!--begin::Label-->
                                <label class="fs-6 fw-bold mb-2">Telefon-2</label>
                                <!--end::Label-->
                                <!--begin::Input-->
                                <input type="text" asp-for="CurrentAccount.Phone2" class="form-control form-control-solid" placeholder="Telefon no giriniz" />
                                <!--end::Input-->
                            </div>
                            <!--end::Input group Phone2-->
                        </div>
                        <!--end::Input group Phone1 & Phone2-->
                        <!--begin::Input group Email & CompanyStatusId-->
                        <div class="row mb-7">
                            <!--begin::Input group Email-->
                            <div class="col-md-6 fv-row">
                                <!--begin::Label-->
                                <label class="required fs-6 fw-bold mb-2">Eposta</label>
                                <!--end::Label-->
                                <!--begin::Input-->
                                <input type="text" asp-for="CurrentAccount.Email" class="form-control form-control-solid" placeholder="Eposta giriniz" />
                                <!--end::Input-->
                            </div>
                            <!--end::Input group Email-->
                            <!--begin::Col CompanyStatusId-->
                            <div class="col-md-6 fv-row">
                                <!--begin::Label-->
                                <label class="required fs-6 fw-bold mb-2">Firma Statüsü</label>
                                <!--end::Label-->
                                <!--begin::input-->
                                <select asp-for="CurrentAccount.CompanyStatusId" asp-items="Html.GetEnumSelectList<CompanyStatusEnum>()" class="form-select form-select-solid fw-bolder" aria-label="Firma Statüsünü Seç." data-control="select2" data-dropdown-parent="#kt_modal_add_subcontractors" data-placeholder="- Lütfen firma statü seçiniz">
                                    <option value="">Firma statüsünü seç...</option>
                                </select>
                                <!--end::input-->
                            </div>
                            <!--end::Input group CompanyStatusId-->
                        </div>
                        <!--end::Input group Email & CompanyStatusId-->
                        <!--begin::Input group BankId IBAN_Code & IBAN-->
                        <div class="row mb-7">
                            <!--begin::Input group BankId-->
                            <div class="col-md-6 fv-row">
                                <!--begin::Label-->
                                <label class="fs-6 fw-bold mb-2">Banka</label>
                                <!--end::Label-->
                                <!--begin::Input-->
                                <input type="text" asp-for="CurrentAccount.BankName" class="form-control form-control-solid" placeholder="Banka adı girin" />
                                <!--end::Input-->
                            </div>
                            <!--end::Input group BankId-->
                            <!--begin::Input group IBAN-->
                            <div class="col-md-6 fv-row">
                                <!--begin::Label-->
                                <label class="fs-6 fw-bold mb-2">IBAN</label>
                                <!--end::Label-->
                                <!--begin::Input-->
                                <input type="text" asp-for="CurrentAccount.IBAN" class="form-control form-control-solid" placeholder="IBAN giriniz" />
                                <!--end::Input-->
                            </div>
                            <!--end::Input group IBAN-->
                        </div>
                        <!--end::Input group BankId & IBAN_Code & IBAN-->
                    </div>
                    <!--end::Scroll-->
                </div>
                <!--end::Modal body-->
                <!--begin::Modal footer-->
                <div class="modal-footer flex-center">
                    <!--begin::Button-->
                    <button type="reset" id="kt_modal_add_subcontractors_cancel" class="btn btn-white me-3">Vazgeç</button>
                    <!--end::Button-->
                    <!--begin::Button-->
                    <button type="submit" id="kt_modal_add_subcontractors_submit" class="btn btn-primary">
                        <span class="indicator-label">Kaydet</span>
                        <span class="indicator-progress">
                            Lütfen bekleyin...
                            <span class="spinner-border spinner-border-sm align-middle ms-2"></span>
                        </span>
                    </button>
                    <!--end::Button-->
                </div>
                <!--end::Modal footer-->
            </form>
            <!--end::Form-->
        </div>
    </div>
</div>
