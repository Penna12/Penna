                                <!--begin::Input-->
                                <select asp-for="CurrentAccount.TownId" asp-items="Model.TownList" class="form-select form-select-solid fw-bolder" aria-label="İlçe Seç." data-control="select2" data-dropdown-parent="#kt_modal_add_subcontractors" data-placeholder="- Lütfen bir ilçe seçiniz">
                                    <option value="">İlçe seç...</option>
                                </select>
                                <!--end::Input-->
                            </div>
                            <!--end::Col TownId-->
                            <!--begin::Col BusinessGroupId-->
                            <div class="col-md-6 fv-row">
                                <!--begin::Label-->
                                <label class="required fs-6 fw-bold mb-2">İş Grubu</label>
                                <!--end::Label-->
                                <!--begin::Input-->
                                <select asp-for="CurrentAccount.BusinessGroupId" asp-items="@Html.GetEnumSelectList<BusinessGroupEnum>()" class="form-select form-select-solid fw-bolder" aria-label="İş grubu Seç." data-control="select2" data-dropdown-parent="#kt_modal_add_subcontractors" data-placeholder="- Lütfen iş grubu seçiniz">
                                    <option value="">İş grubu seç...</option>
                                </select>
                                <!--end::Input-->
                            </div>
                            <!--end::Col BusinessGroupId-->
                        </div>
                        <!--end::Input group TownId & BusinessGroupId-->
                        <!--begin::Input group TaxId & TaxOffice-->
                        <div class="row g-9 mb-7">
                            <!--begin::Col TaxId-->
                            <div class="col-md-6 fv-row">
                                <!--begin::Label-->
                                <label class="required fs-6 fw-bold mb-2">Vergi No</label>
