                    <div class="scroll-y me-n7 pe-7" id="kt_modal_add_subcontractors_scroll" data-kt-scroll="true" data-kt-scroll-activate="{default: false, lg: true}" data-kt-scroll-max-height="auto" data-kt-scroll-dependencies="#kt_modal_add_subcontractors_header" data-kt-scroll-wrappers="#kt_modal_add_subcontractors_scroll" data-kt-scroll-offset="300px">
                        <!--begin::Input group CompanyName & AuthorizedPerson-->
                        <div class="row mb-7">
                            <!--begin::Input group CompanyName-->
                            <div class="col-md-6 fv-row">
                                <!--begin::Label-->
                                <label class="required fs-6 fw-bold mb-2">Taşeron Firma Adı</label>
                                <!--end::Label-->
                                <!--begin::Input-->
                                <input type="text" asp-for="CurrentAccount.CompanyName" class="form-control form-control-solid" placeholder="Taşeron firma adı giriniz" />
                                <!--end::Input-->
                            </div>
                            <!--end::Input group CompanyName-->
                            <!--begin::Input group AuthorizedPerson-->
                            <div class="col-md-6 fv-row">
                                <!--begin::Label-->
                                <label class="required fs-6 fw-bold mb-2">Yetkili Kişi</label>
                                <!--end::Label-->
                                <!--begin::Input-->
                                <input type="text" asp-for="CurrentAccount.AuthorizedPerson" class="form-control form-control-solid" placeholder="Yetkili kişiyi giriniz" />
                                <!--end::Input-->
                            </div>
                            <!--end::Input group AuthorizedPerson-->
                        </div>
                        <!--end::Input group CompanyName & AuthorizedPerson-->
                        <!--begin::Input group Address-->
                        <div class="row mb-7">
                            <div class="col-md-12 fv-row">
                                <!--begin::Label-->
                                <label class="required fs-6 fw-bold mb-2">Adres</label>
                                <!--end::Label-->
                                <!--begin::Input Address-->
                                <textarea asp-for="CurrentAccount.Address" class="form-control form-control-solid" placeholder="Adres giriniz"></textarea>
                                <!--end::Input Address-->
                            </div>
                        </div>
                        <!--end::Input group Address-->
                        <!--begin::Input group City & Country-->
                        <div class="row g-9 mb-7">
                            <!--begin::Col Country-->
                            <div class="col-md-6 fv-row">
                                <!--begin::Label-->
                                <label class="fs-6 fw-bold mb-2">
                                    <span class="required">Ülke</span>
                                    <i class="fas fa-exclamation-circle ms-1 fs-7" data-bs-toggle="tooltip" title="Ülke"></i>
                                </label>
                                <!--end::Label-->
                                <!--begin::Input country-->
                                <select asp-for="CurrentAccount.CountryId" asp-items="Model.CountryList" class="form-select form-select-solid fw-bolder" aria-label="Ülke Seç." data-control="select2" data-dropdown-parent="#kt_modal_add_subcontractors" data-placeholder="- Lütfen bir ülke seçiniz">
                                    <option value="">Ülke seç...</option>
                                </select>
                                <!--end::Input country-->
                            </div>
                            <!--end::Col Country-->
                            <!--begin::Col City-->
                            <div class="col-md-6 fv-row">
                                <!--begin::Label-->
                                <label class="required fs-6 fw-bold mb-2">Şehir</label>
                                <!--end::Label-->
                                <!--begin::Input-->
                                <select asp-for="CurrentAccount.CityId" asp-items="Model.CityList" class="form-select form-select-solid fw-bolder" aria-label="Şehir Seç." data-control="select2" data-dropdown-parent="#kt_modal_add_subcontractors" data-placeholder="- Lütfen bir şehir seçiniz">
                                    <option value="">Şehir seç...</option>
                                </select>
                                <!--end::Input-->
                            </div>
                            <!--end::Col City-->
                        </div>
                        <!--end::Input group CityId & CountryId-->
                        <!--begin::Input group TownId & BusinessGroupId-->
                        <div class="row g-9 mb-7">
                            <!--begin::Col TownId-->
                            <div class="col-md-6 fv-row">
                                <!--begin::Label-->
                                <label class="fs-6 fw-bold mb-2">
                                    <span class="required">İlçe</span>
                                    <i class="fas fa-exclamation-circle ms-1 fs-7" data-bs-toggle="tooltip" title="İlçe"></i>
                                </label>
