
@{
    ViewData["Title"] = "Proje Detayları";
}

<div class="card">
    <div class="card-header border-0 pt-10">
        <h2 class="fw-bolder"><span>Proje Detayları</span></h2>
    </div>
    <div class="card-body">
        <div class="row">
            <div class="col-md-4">
                <label>Proje ID:</label>
                <span>@User.GetClaimValue("ProjectId")</span>
            </div>
            <div class="col-md-4">
                <label>Proje Adı:</label>
                <span> @User.GetClaimValue("ProjectName")</span>
            </div>
            <div class="col-md-4">
                <label>Şirket:</label>
                <span> @User.GetClaimValue("TenantName")</span>
            </div>
        </div>
    </div>
    <div class="card-footer">

    </div>
</div>