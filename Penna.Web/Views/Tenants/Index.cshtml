                        <div class="fv-row mb-7">
                            <!--begin::Label-->
                            <label class="required fs-6 fw-bold mb-2">Ünvan</label>
                            <!--end::Label-->
                            <!--begin::Input-->
                            <input type="text" asp-for="Title" class="form-control form-control-solid" placeholder="Ünvan girinizi" />
                            <!--end::Input-->
                        </div>
                        <!--end::Input group Title-->
                        <!--begin::Input group FullName-->
                        <div class="fv-row mb-7">
                            <!--begin::Label-->
                            <label class="required fs-6 fw-bold mb-2">Ad Soyad</label>
                            <!--end::Label-->
                            <!--begin::Input-->
                            <input type="text" asp-for="FullName" class="form-control form-control-solid" placeholder="Ad Soyad giriniz" />
                            <!--end::Input-->
                        </div>
                        <!--end::Input group FullName-->
                        <!--begin::Input group Email-->
                        <div class="fv-row mb-7">
                            <!--begin::Label-->
                            <label class="fs-6 fw-bold mb-2">
                                <span class="required">Email</span>
                                <i class="fas fa-exclamation-circle ms-1 fs-7" data-bs-toggle="tooltip" title="Eposta hesabı aktifleştirmek için gereklidir"></i>
                            </label>
                            <!--end::Label-->
                            <!--begin::Input-->
                            <input type="text" asp-for="Email" class="form-control form-control-solid" placeholder="Eposta giriniz" />
                            <!--end::Input-->
                        </div>
                        <!--end::Input group Email-->
                        <!--begin::Input group PhoneNumber-->
                        <div class="fv-row mb-15">
                            <!--begin::Label-->
                            <label class="required fs-6 fw-bold mb-2">Telefon</label>
                            <!--end::Label-->
                            <!--begin::Input-->
                            <input type="text" asp-for="Phone" class="form-control form-control-solid" data-inputmax="(999) 999-9999" placeholder="Telefon no giriniz" />
                            <!--end::Input-->
                        </div>
                        <!--end::Input group PhoneNumber-->
                        <!--begin::Input group TaxId & TaxOffice-->
                        <div class="row g-9 mb-7">
                            <!--begin::Col TaxId-->
                            <div class="col-md-6 fv-row">
                                <!--begin::Label-->
                                <label class="fs-6 fw-bold mb-2">
                                    <span class="required">Vergi No</span>
                                    <i class="fas fa-exclamation-circle ms-1 fs-7" data-bs-toggle="tooltip" title="Vergi No veya TC Kimlik No girmelisiniz"></i>
                                </label>
                                <!--end::Label-->
                                <!--begin::Input-->
                                <input asp-for="TaxId" class="form-control form-control-solid" maxlength="11" placeholder="Vergi No/TC Kimlik No" />
                                <!--end::Input-->
                            </div>
                            <!--end::Col TaxId-->
                            <!--begin::Col TaxOffice-->
                            <div class="col-md-6 fv-row">
                                <!--begin::Label-->
                                <label class="fs-6 fw-bold mb-2"> Vergi Dairesi </label>
                                <!--end::Label-->
                                <!--begin::Input taxOffice-->
                                <input asp-for="TaxOffice" class="form-control form-control-solid" placeholder="" />
                                <!--end::Input taxOffice-->
                            </div>
                            <!--end::Col TaxOffice-->
                        </div>
                        <!--end::Input group-->
                        <!--begin::Address toggle-->
                        <div class="fw-bolder fs-3 rotate collapsible mb-7" data-bs-toggle="collapse" href="#kt_modal_add_tenants_address_info" role="button" aria-expanded="false" aria-controls="kt_customer_view_details">
                            Adres Bilgisi
                            <span class="ms-2 rotate-180">
                                <!--begin::Svg Icon | path: icons/duotone/Navigation/Angle-down.svg-->
                                <span class="svg-icon svg-icon-3">
                                    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24px" height="24px" viewBox="0 0 24 24" version="1.1">
                                        <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                                            <polygon points="0 0 24 0 24 24 0 24" />
                                            <path d="M6.70710678,15.7071068 C6.31658249,16.0976311 5.68341751,16.0976311 5.29289322,15.7071068 C4.90236893,15.3165825 4.90236893,14.6834175 5.29289322,14.2928932 L11.2928932,8.29289322 C11.6714722,7.91431428 12.2810586,7.90106866 12.6757246,8.26284586 L18.6757246,13.7628459 C19.0828436,14.1360383 19.1103465,14.7686056 18.7371541,15.1757246 C18.3639617,15.5828436 17.7313944,15.6103465 17.3242754,15.2371541 L12.0300757,10.3841378 L6.70710678,15.7071068 Z" fill="#000000" fill-rule="nonzero" transform="translate(12.000003, 11.999999) rotate(-180.000000) translate(-12.000003, -11.999999)" />
                                        </g>
                                    </svg>
                                </span>
                                <!--end::Svg Icon-->
                            </span>
                        </div>
                        <!--end::Address toggle-->
                        <!--begin::Address form-->
                        <div id="kt_modal_add_tenants_address_info" class="collapse show">
                            <!--begin::Input group Address-->
                            <div class="d-flex flex-column mb-7 fv-row">
                                <!--begin::Label-->
                                <label class="fs-6 fw-bold mb-2">Adres</label>
                                <!--end::Label-->
                                <!--begin::Input-->
                                <textarea asp-for="Address" class="form-control form-control-solid" placeholder="" rows="3"></textarea>
                                <!--end::Input-->
                            </div>
                            <!--end::Input group Address-->
                            <!--begin::Input group City & Country-->
                            <div class="row g-9 mb-7">
                                <!--begin::Col Country-->
                                <div class="col-md-6 fv-row">
                                    <!--begin::Label-->
                                    <label class="fs-6 fw-bold mb-2">
                                        <span class="required">Ülke</span>
                                        <i class="fas fa-exclamation-circle ms-1 fs-7" data-bs-toggle="tooltip" title="Ülke"></i>
                                    </label>
                                    <!--end::Label-->
                                    <!--begin::Input country-->
                                    <select asp-for="CountryId" asp-items="Model.CountryList" class="form-select form-select-solid fw-bolder" aria-label="Ülke Seç." data-control="select2" data-dropdown-parent="#kt_modal_add_tenants" data-placeholder="- Lütfen bir ülke seçiniz">
                                        <option value="">Ülke seç...</option>
                                    </select>

                                    <!--end::Input country-->
                                </div>
                                <!--end::Col Country-->
                                <!--begin::Col City-->
                                <div class="col-md-6 fv-row">
                                    <!--begin::Label-->
                                    <label class="required fs-6 fw-bold mb-2">Şehir</label>
                                    <!--end::Label-->
                                    <!--begin::Input-->
                                    <select asp-for="CityId" asp-items="Model.CityList" class="form-select form-select-solid fw-bolder" aria-label="Şehir Seç." data-control="select2" data-dropdown-parent="#kt_modal_add_tenants" data-placeholder="- Lütfen bir şehir seçiniz">
                                        <option value="">Şehir seç...</option>
                                    </select>
                                    <!--end::Input-->
                                </div>
                                <!--end::Col City-->
                            </div>
                            <!--end::Input group-->
                        </div>
                        <!--end::Address form-->
                    </div>
                    <!--end::Scroll-->
                </div>
                <!--end::Modal body-->
                <!--begin::Modal footer-->
                <div class="modal-footer flex-center">
                    <!--begin::Button-->
                    <button type="reset" id="kt_modal_add_tenants_cancel" class="btn btn-white me-3">Vazgeç</button>
                    <!--end::Button-->
                    <!--begin::Button-->
                    <button type="submit" id="kt_modal_add_tenants_submit" class="btn btn-primary">
                        <span class="indicator-label">Kaydet</span>
                        <span class="indicator-progress">
                            Lütfen bekleyin...
                            <span class="spinner-border spinner-border-sm align-middle ms-2"></span>
                        </span>
                    </button>
                    <!--end::Button-->
                </div>
                <!--end::Modal footer-->
            </form>
            <!--end::Form-->
        </div>
    </div>
</div>
<!--end::Modal - Tenant - Add-->
<!--begin::Modal - Tenant - Edit-->
<div class="modal fade" id="kt_modal_edit_tenants" tabindex="-1" aria-hidden="true">
